        <script src="assets/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
        <script type='text/javascript' src='assets/js/jquery.placeholder.min.js'></script>
        <script src="assets/jquery.sticky.js" type="text/javascript"></script>
        <script src="assets/jquery.flexslider-min.js" type="text/javascript"></script>
        <script src="assets/jquery.mixitup.min.js" type="text/javascript"></script>
        <script src="assets/parallax.min.js" type="text/javascript"></script>
        <script src="assets/wow.min.js" type="text/javascript"></script>
        <script src="assets/app.js" type="text/javascript"></script>
        <script src="assets/mailchimp-custom.js" type="text/javascript"></script>
