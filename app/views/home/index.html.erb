                </div>
            </div>
        </section> <!--testimonials section end here-->
        <div class="space-70"></div>

        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h3 class="heading-sec">Latest news</h3>
                </div>
            </div><!--row-->
            <div class="row">
                <% @posts.first(3).each do |post| %>
                <div class="col-md-4 col-sm-6 margin-btm-20">
                    <div class="news-sec">
                        <div class="news-thumnail">
                            <h3 class="blog-post-title">
                                <%= post.title %>
                            </h3>
                            <span class="news-post-cat"><%= post.created_at.strftime("%B %d, %Y") %></span>
                            <p><%= post.content %></p>
                        </div>
                    </div>
                </div><!--latest news items-->
                <% end %>
            </div><!--row end-->
        </div><!--container recent news end-->
        <div class="space-50"></div>
        </div>
        <div class="cta-bg">
            <div class="container text-center">
                <h3>貼心、溫暖、像家
                    <a href="https://linyoga.typeform.com/to/Cry25x", target = "_blank", class="btn btn-lg btn-white btn-radius">開始預約</a>
                </h3>
            </div>
        </div>

         <!--scripts-->
        <script src="assets/jquery.min.js" type="text/javascript"></script>
        <script type="text/javascript" src="assets/js/modernizr.custom.97074.js"></script>
        <script src="assets/bootstrap.min.js" type="text/javascript"></script>
        <script src="assets/jquery.easing.1.3.min.js" type="text/javascript"></script>
